# In[11]:


def build_kanemele_ribbon(k_y, t1=1.0, lambda_SO=0.3, N_x=40):
    """
    Build Kane-Mele ribbon Hamiltonian:
    - Open boundary in x (finite width)
    - Periodic in y → momentum k_y
    - Spin-orbit coupling included
    """
    N_sites_per_spin = 2 * N_x  # A and B per unit cell
    H = np.zeros((2*N_sites_per_spin, 2*N_sites_per_spin), dtype=complex)

    def idx(x, sub, spin):
        base = 2*x + sub
        return base + (0 if spin == 0 else N_sites_per_spin)  # spin up: 0, spin down: 1

    for x in range(N_x):
        iA_up = idx(x, 0, 0); iB_up = idx(x, 1, 0)
        iA_dn = idx(x, 0, 1); iB_dn = idx(x, 1, 1)

        # On-site: none (no staggered potential)

        # NN hopping (A-B): spin-independent
        H[iA_up, iB_up] += -t1
        H[iB_up, iA_up] += -t1
        H[iA_dn, iB_dn] += -t1
        H[iB_dn, iA_dn] += -t1

        # δ2: (-1, 0) bond
        if x > 0:
            jB_up = idx(x-1, 1, 0); jB_dn = idx(x-1, 1, 1)
            phase = np.exp(-1j * k_y)
            H[iA_up, jB_up] += -t1 * phase
            H[jB_up, iA_up] += -t1 * np.conj(phase)
            H[iA_dn, jB_dn] += -t1 * phase
            H[jB_dn, iA_dn] += -t1 * np.conj(phase)

        # NNN hopping (A-A and B-B) with spin-orbit
        # Clockwise (ν = +1): contributes +iλ for up, -iλ for down
        if x < N_x - 1:
            jA_up = idx(x+1, 0, 0); jA_dn = idx(x+1, 0, 1)
            H[iA_up, jA_up] += 1j * lambda_SO
            H[jA_up, iA_up] += -1j * lambda_SO
            H[iA_dn, jA_dn] += -1j * lambda_SO
            H[jA_dn, iA_dn] += 1j * lambda_SO

        if x > 0:
            jA_up = idx(x-1, 0, 0); jA_dn = idx(x-1, 0, 1)
            H[iA_up, jA_up] += -1j * lambda_SO
            H[jA_up, iA_up] += 1j * lambda_SO
            H[iA_dn, jA_dn] += 1j * lambda_SO
            H[jA_dn, iA_dn] += -1j * lambda_SO

        # B-B NNN hops
        if x < N_x - 1:
            jB_up = idx(x+1, 1, 0); jB_dn = idx(x+1, 1, 1)
            H[iB_up, jB_up] += -1j * lambda_SO
            H[jB_up, iB_up] += 1j * lambda_SO
            H[iB_dn, jB_dn] += 1j * lambda_SO
            H[jB_dn, iB_dn] += -1j * lambda_SO

        if x > 0:
            jB_up = idx(x-1, 1, 0); jB_dn = idx(x-1, 1, 1)
            H[iB_up, jB_up] += 1j * lambda_SO
            H[jB_up, iB_up] += -1j * lambda_SO
            H[iB_dn, jB_dn] += -1j * lambda_SO
            H[jB_dn, iB_dn] += 1j * lambda_SO

    return H


# In[12]:


# Parameters
t1 = 1.0           # Nearest-neighbor hopping
lambda_SO = 0.3    # Spin-orbit coupling strength
N_x = 40           # Number of unit cells in x (width)
k_y = 1.0          # Fixed transverse momentum
k_vals = np.linspace(-np.pi, np.pi, 100)  # For band structure


# In[13]:


# Build Hamiltonian with default parameters
H = build_kanemele_ribbon(k_y=k_y, t1=t1, lambda_SO=lambda_SO, N_x=N_x)

# Diagonalize
eigenvalues, eigenvectors = np.linalg.eigh(H)

# Sort by energy
idx = np.argsort(eigenvalues)
eigenvalues = eigenvalues[idx]

# Plot: Eigenvalues vs Index
plt.figure(figsize=(10, 6))
plt.plot(eigenvalues, '-b', markersize=3)

plt.xlabel('State Index')
plt.ylabel('Energy $E$')
plt.title(f'Kane-Mele Model: Eigenvalues vs Index\n'
          f'$N_x={N_x}, k_y={k_y:.2f}, t_1={t1}, \\lambda_{{\\mathrm{{SO}}}}={lambda_SO}$')
plt.grid(True, alpha=0.3)
plt.tight_layout()
plt.show()


# In[14]:


# Compute spectrum E(k_y)
bands = []
for ky in k_vals:
    H = build_kanemele_ribbon(k_y=ky, t1=t1, lambda_SO=lambda_SO, N_x=N_x)
    eigvals = np.linalg.eigvalsh(H)
    bands.append(np.sort(eigvals))

bands = np.array(bands)

# Plot Band Structure
plt.figure(figsize=(8, 6))

# Plot all bands
for n in range(bands.shape[1]):

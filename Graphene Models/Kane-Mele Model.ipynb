# Energy dispersion for spin σ
def energy_plus_km(kx, ky, t1_val, lambda_SO_val, a_val):
    # Compute d_x, d_y, d_z_SO
    k = np.array([kx, ky])
    dx = -t1_val * (np.cos(k @ [0, -a_val]) + 
                    np.cos(k @ [np.sqrt(3)/2*a_val, 0.5*a_val]) + 
                    np.cos(k @ [-np.sqrt(3)/2*a_val, 0.5*a_val]))
    dy = -t1_val * (np.sin(k @ [0, -a_val]) + 
                    np.sin(k @ [np.sqrt(3)/2*a_val, 0.5*a_val]) + 
                    np.sin(k @ [-np.sqrt(3)/2*a_val, 0.5*a_val]))
    dz = 2 * lambda_SO_val * (np.sin(k @ [np.sqrt(3)*a_val, 0]) + 
                              np.sin(k @ [-np.sqrt(3)/2*a_val, -1.5*a_val]) + 
                              np.sin(k @ [-np.sqrt(3)/2*a_val, 1.5*a_val]))
    return np.sqrt(dx**2 + dy**2 + dz**2)

def energy_minus_km(kx, ky, t1_val, lambda_SO_val, a_val):
    return -energy_plus_km(kx, ky, t1_val, lambda_SO_val, a_val)



# In[17]:


a = 1.0  # lattice constant

# direct lattice vectors for graphene (conventional choice)
a1 = np.array([np.sqrt(3)/2 * a,  3/2 * a])
a2 = np.array([-np.sqrt(3)/2 * a, 3/2 * a])

# reciprocal primitive vectors b1, b2 satisfying a_i·b_j = 2π δ_ij
area = a1[0]*a2[1] - a1[1]*a2[0]
b1 = 2*np.pi * np.array([ a2[1], -a2[0] ]) / area
b2 = 2*np.pi * np.array([ -a1[1], a1[0] ]) / area

# nearest neighbours (A->B) (these are conventional)
delta1 = np.array([0, -a])
delta2 = np.array([np.sqrt(3)/2 * a, a/2])
delta3 = np.array([-np.sqrt(3)/2 * a, a/2])
deltas = [delta1, delta2, delta3]

# next-nearest neighbor vectors used in SOC term (A->A)
b_vecs = [
    np.array([np.sqrt(3)*a, 0]),
    np.array([-np.sqrt(3)/2*a, -3*a/2]),
    np.array([-np.sqrt(3)/2*a,  3*a/2])
]


# In[18]:


def h_k(kx, ky, t1=1.0, lambda_SO=0.3):
    """2x2 Bloch Hamiltonian for a single spin (Kane-Mele block)."""
    dk_sum = sum(np.exp(1j * (kx*d[0] + ky*d[1])) for d in deltas)
    dx = -t1 * np.real(dk_sum)
    dy = -t1 * np.imag(dk_sum)
    fz = sum(np.sin(kx*b[0] + ky*b[1]) for b in b_vecs)
    dz = 2.0 * lambda_SO * fz

    sx = np.array([[0, 1], [1, 0]], dtype=complex)
    sy = np.array([[0, -1j], [1j, 0]], dtype=complex)
    sz = np.array([[1, 0], [0, -1]], dtype=complex)

    return dx*sx + dy*sy + dz*sz


# In[19]:


def compute_chern_number_cell(h_func, nk=41, gap_tol=1e-8, return_gap=False):
    """
    Compute Chern number on the primitive cell using midpoint sampling to avoid
    sampling Dirac points exactly. Also computes min gap and raises a warning
    if gap < gap_tol (in which case the Chern is unreliable).
    """
    # midpoint sampling in u, v ∈ [0,1)
    us = (np.arange(nk) + 0.5) / nk
    vs = (np.arange(nk) + 0.5) / nk

    psi = np.zeros((nk, nk, 2), dtype=complex)
    min_gap = np.inf

    for i, u in enumerate(us):
        for j, v in enumerate(vs):
            k = u*b1 + v*b2
            H = h_func(k[0], k[1])
            evals, evecs = np.linalg.eigh(H)
            # store valence eigenvector
            psi[i, j, :] = evecs[:, 0]
            # update gap (conduction - valence)
            gap = evals[1] - evals[0]
            if gap < min_gap:
                min_gap = gap

    if min_gap < gap_tol:
        # Gap too small — Chern unreliable. Return a special flag or still compute but warn.
        # We'll continue to compute but caller should inspect min_gap.
        print(f"Warning: min band gap = {min_gap:.3e} < gap_tol ({gap_tol}). Chern may be ill-defined.")

    # build normalized link variables
    Ux = np.zeros((nk, nk), dtype=complex)
    Uy = np.zeros((nk, nk), dtype=complex)
    eps = 1e-12
    for i in range(nk):
        for j in range(nk):
            ip1 = (i + 1) % nk
            jp1 = (j + 1) % nk
            ux = np.vdot(psi[i, j], psi[ip1, j])
            uy = np.vdot(psi[i, j], psi[i, jp1])
            Ux[i, j] = ux / (np.abs(ux) + eps)
            Uy[i, j] = uy / (np.abs(uy) + eps)

    # plaquette phases
    F = np.zeros((nk, nk))
    for i in range(nk):
        for j in range(nk):
            ip1 = (i + 1) % nk
            jp1 = (j + 1) % nk
            phase = Ux[i, j] * Uy[ip1, j] * np.conj(Ux[i, jp1]) * np.conj(Uy[i, j])
            F[i, j] = np.angle(phase)

    chern = np.sum(F) / (2 * np.pi)
    chern_int = int(np.rint(chern))

    if return_gap:
        return chern_int, chern, min_gap
    else:
        return chern_int


# In[20]:


def compute_z2(t1=1.0, lambda_SO=0.3, nk=41):
    def h_up(kx, ky):
        return h_k(kx, ky, t1=t1, lambda_SO=lambda_SO)
    def h_dn(kx, ky):
        return h_k(kx, ky, t1=t1, lambda_SO=-lambda_SO)

    C_up = compute_chern_number_cell(h_up, nk=nk)
    C_dn = compute_chern_number_cell(h_dn, nk=nk)
    # For Kane-Mele, time-reversal gives C_dn = -C_up (ideally); Z2 invariant is |C_up| mod 2
    nu = abs(C_up) % 2
    return nu, C_up, C_dn



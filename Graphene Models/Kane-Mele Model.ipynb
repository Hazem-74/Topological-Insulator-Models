def interpolate_path(points, n_per_segment=100):
    kx_vals, ky_vals = [], []
    x_ticks = [0]
    cumulative = 0
    for i in range(len(points)-1):
        start, end = points[i], points[i+1]
        kxs = np.linspace(start[0], end[0], n_per_segment)
        kys = np.linspace(start[1], end[1], n_per_segment)
        kx_vals.extend(kxs)
        ky_vals.extend(kys)
        cumulative += n_per_segment
        x_ticks.append(cumulative)
    return np.array(kx_vals), np.array(ky_vals), x_ticks

kx_path, ky_path, x_ticks = interpolate_path(path_points)

# Evaluate energies along path
E_plus_path = [energy_plus_km(kx, ky, t1_val, lambda_SO_val, a_val) for kx, ky in zip(kx_path, ky_path)]
E_minus_path = [energy_minus_km(kx, ky, t1_val, lambda_SO_val, a_val) for kx, ky in zip(kx_path, ky_path)]



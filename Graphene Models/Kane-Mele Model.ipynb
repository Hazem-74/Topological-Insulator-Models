# In[2]:


# Define symbols
kx, ky, t1, lambda_SO = sp.symbols('k_x k_y t1 lambda_SO', real=True)

# Lattice constant
a = sp.symbols('a', real=True, positive=True)

# Pauli matrices
sigma_x = sp.Matrix([[0, 1], [1, 0]])
sigma_y = sp.Matrix([[0, -sp.I], [sp.I, 0]])
sigma_z = sp.Matrix([[1, 0], [0, -1]])

# Nearest-neighbor vectors
a1 = sp.Matrix([0, -a])
a2 = sp.Matrix([sp.sqrt(3)/2 * a, sp.Rational(1,2) * a])
a3 = sp.Matrix([-sp.sqrt(3)/2 * a, sp.Rational(1,2) * a])
a_vecs = [a1, a2, a3]

# d_x and d_y (same for both spins)
d_x = -t1 * sum(sp.cos(kx*v[0] + ky*v[1]) for v in a_vecs)
d_y = -t1 * sum(sp.sin(kx*v[0] + ky*v[1]) for v in a_vecs)

# Next-nearest-neighbor vectors
b1 = sp.Matrix([sp.sqrt(3) * a, 0])
b2 = sp.Matrix([-sp.sqrt(3)/2 * a, -sp.Rational(3,2) * a])
b3 = sp.Matrix([-sp.sqrt(3)/2 * a,  sp.Rational(3,2) * a])
b_vecs = [b1, b2, b3]

# Spin-orbit contribution to d_z
d_z_SO = 2 * lambda_SO * sum(sp.sin(kx*v[0] + ky*v[1]) for v in b_vecs)

# Total Hamiltonian for spin up and down
H_up = d_x * sigma_x + d_y * sigma_y + d_z_SO * sigma_z
H_down = d_x * sigma_x + d_y * sigma_y - d_z_SO * sigma_z

# Full 4x4 Hamiltonian (block diagonal)
H_full = sp.Matrix([
    [H_up[0,0], H_up[0,1], 0, 0],
    [H_up[1,0], H_up[1,1], 0, 0],
    [0, 0, H_down[0,0], H_down[0,1]],
    [0, 0, H_down[1,0], H_down[1,1]]
])

# Simplify
H_full = sp.simplify(H_full)
H_up = sp.simplify(H_up)
H_down = sp.simplify(H_down)


